<UserControl x:Class="ChamCong365.Popup.DatePicker.ucLoadMonths"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChamCong365.Popup.DatePicker"
             mc:Ignorable="d" >
    <Grid Margin="0 0 0 5" Width="47" Height="22">
        <Border x:Name="bodNumberMonth"
            BorderThickness="0"
            MouseUp="bodNumberMonth_MouseUp">
            <TextBlock  x:Name="txbLoadMonth"
                Style="{DynamicResource TextBlockCalenda}"/>
            
        </Border>
    </Grid>
</UserControl>
